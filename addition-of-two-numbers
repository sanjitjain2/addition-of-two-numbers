#include<iostream>
using namespace std;
class maths
{
   int x,y;
   public:
      void getdata();
      void display();
};
void maths::getdat()
{
cout<<"Enter First Number = ";
cin>>x;
cout<<"Enter Second Number = ";
cin>>y;

}
void maths::display()
{

cout<<"Answer is = "<<x+y<<endl;

}
      




int main()
{
cout<<"WELCOME TO OUR PROGRAM"<<endl;
maths m;
m.getdat();
m.display();


return 0;

}
